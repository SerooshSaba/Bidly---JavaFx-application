<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<?import javafx.scene.control.ComboBox?>

<?import javafx.scene.image.ImageView?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml" fx:controller="com.bidly.bidly.ApplicationController">

    <!--<Label fx:id="logoText" text="Bidly" />-->
    <ImageView fx:id="logoImage" />

    <Label text="Login as:" />

    <ComboBox fx:id="userTypes" layoutX="15.0" layoutY="33.0" prefWidth="125.0" value="Bidder" />

    <HBox alignment="CENTER" >
        <Button text="Login" onAction="#LoginClick"/>
    </HBox>

</VBox>
